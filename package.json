{
  "name": "nope-commerce-automation",
  "version": "1.0.0",
  "description": "nope commerce site automation using webdriverio",
  "main": "index.js",
  "scripts": {
    "test": "npx wdio wdio.conf.js",
    "login": "wdio wdio.conf.js --suite login",
    "registration": "wdio wdio.conf.js --suite registration",
    "placeOrder": "wdio wdio.conf.js --suite placeOrder",
    "nopCommerce": "wdio wdio.conf.js --suite nopCommerce",
    "getReport": "allure generate --clean allure-results && allure open"
  },
  "keywords": [
    "webdriverio",
    "chai",
    "mocha",
    "allure",
    "node-xlsx",
    "data-driven"
  ],
  "author": "hridoy_das",
  "license": "ISC",
  "devDependencies": {
    "@wdio/cli": "^8.17.0",
    "@wdio/local-runner": "^8.17.0",
    "@wdio/mocha-framework": "^8.17.0",
    "@wdio/spec-reporter": "^8.17.0"
  },
  "dependencies": {
    "@wdio/allure-reporter": "^8.17.0",
    "allure-commandline": "^2.24.1",
    "chai": "^4.3.10",
    "node-xlsx": "^0.21.0"
  }
}
